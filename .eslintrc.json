{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true,
        "mocha": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": ["warn", { "varsIgnorePattern": "value|user"}]
    },
    "ignorePatterns": ["client/", "documentation/", "lintArtifact/", "dist/", "docs/"]
}
